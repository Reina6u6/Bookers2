<h1>User info</h1>
<%= image_tag 'no_image', size: "200x200" %>

<h4>名前</h4>
<h4>自己紹介文</h4>
<%= link_to 'edit', edit_user_password_path %>

<h1>New book</h1>

<%= form_with model: @book do |f| %>
<h4>本のタイトル</h4>
<%= f.text_field :title %>

<h4>感想</h4>
<%= f.text_area :body %>

<%= f.submit 'create Book' %>
<% end %>