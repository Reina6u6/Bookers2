<h1>User info</h1>
<%= form_with(model: @user, url: user_path, method: :patch) do |f| %>
<h4>名前</h4>
<%= f.text_field :name %>

<h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

<h4>自己紹介</h4>
<%= f.text_area :introduction %>

<%= f.submit 'create Book', name: "create_book"  %>
<% end %>

